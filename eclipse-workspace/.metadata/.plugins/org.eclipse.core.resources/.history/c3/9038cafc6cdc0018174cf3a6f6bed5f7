
public class inCLASS10252018 {
	public static void main (String args[]) {
		int i =0;
		
		for(i=0;i<=20;i++) {
			System.out.print(" "+i);
		}
		System.out.println();
		
		for(i=0;i<=20;i+=2) {
			System.out.print(" "+ i);
		}
		System.out.println();
		
		for(i=0;i<=20;i++) {
			System.out.printf(" "+ ++i);
		}
}
}